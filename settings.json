{
  "editor.fontFamily": "'JetBrainsMono Nerd Font', Menlo, Monaco",
  "editor.fontLigatures": true,
  "editor.glyphMargin": false,
  "editor.suggestSelection": "first",
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.bracketPairColorization.enabled": false,
  "editor.minimap.maxColumn": 75,
  "editor.minimap.showSlider": "always",
  "editor.scrollBeyondLastLine": false,
  "editor.scrollbar.verticalScrollbarSize": 10,
  "editor.scrollbar.vertical": "visible",
  "editor.guides.bracketPairs": true,
  "editor.renderLineHighlightOnlyWhenFocus": true,
  "editor.accessibilitySupport": "off",
  "editor.cursorBlinking": "phase",
  "editor.find.cursorMoveOnType": false,
  "editor.cursorStyle": "block",
  "files.associations": {
    ".skhdrc": "shellscript",
    ".yabairc": "shellscript"
  },
  "files.autoSave": "onWindowChange",
  "files.trimTrailingWhitespace": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.autoReveal": "focusNoScroll",
  "window.titleBarStyle": "native",
  "window.autoDetectColorScheme": true,
  "workbench.preferredLightColorTheme": "GitHub Light",
  "workbench.preferredDarkColorTheme": "GitHub Dark",
  "workbench.editor.tabCloseButton": "off",
  "workbench.editor.pinnedTabSizing": "compact",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.tree.indent": 10,
  "workbench.startupEditor": "none",
  "workbench.activityBar.visible": false,
  "workbench.colorCustomizations": {
    // "tab.activeBackground": "#0fd9e3"
  },
  "search.actionsPosition": "right",
  "search.exclude": {
    "**/dist": true
  },
  "search.showLineNumbers": true,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "terminal.integrated.fontFamily": "\"MesloLGS NF\"",
  "terminal.integrated.fontSize": 8,
  "terminal.integrated.enablePersistentSessions": true,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.tabs.enabled": false,
  "terminal.integrated.cursorBlinking": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "gitlens.defaultGravatarsStyle": "retro",
  "gitlens.hovers.annotations.over": "annotation",
  "gitlens.hovers.avatarSize": 48,
  "gitlens.hovers.changesDiff": "hunk",
  "gitlens.currentLine.format": "${author, }${agoOrDate}${' via 'pullRequest}",
  "gitlens.views.showRelativeDateMarkers": false,
  "gitlens.changes.locations": ["gutter"],
  "scm.diffDecorationsGutterWidth": 2,
  "cSpell.userWords": [
    "adhoc",
    "discoverability",
    "fastmail",
    "frameless",
    "hocs",
    "Interactor",
    "jsdelivr",
    "lvadla",
    "mantine",
    "mediapipe",
    "Menlo",
    "Meslo",
    "Multiselect",
    "pexip",
    "preso",
    "SGMT",
    "spacebar",
    "stylelint",
    "tabbable",
    "tabler",
    "testid",
    "unstaged",
    "vadla",
    "visx",
    "vite",
    "wirelessly"
  ],
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "eslint.alwaysShowStatus": true,
  "eslint.codeActionsOnSave.mode": "problems",
  "eslint.debug": true,
  "eslint.run": "onSave",
  "eslint.packageManager": "yarn",
  "workbench.colorTheme": "GitHub Dark",
  "testing.defaultGutterClickAction": "contextMenu",
  "gitlens.views.repositories.showStashes": false,
  "gitlens.views.repositories.showTags": false,
  "gitlens.views.repositories.showContributors": false,
  "gitlens.heatmap.toggleMode": "window",
  "update.mode": "manual",
  "telemetry.telemetryLevel": "off",
  "git.confirmSync": false,
  "scm.inputFontSize": 12,
  "stylelint.validate": ["css", "less", "postcss", "scss"],
  "stylelint.snippet": ["css", "less", "postcss", "scss"],
  "stylelint.reportNeedlessDisables": true,
  "customizeUI.titleBar": "frameless",
  "editor.minimap.renderCharacters": false,
  "editor.minimap.side": "left",
  "audioCues.taskCompleted": "on",
  "audioCues.taskFailed": "on"
}
