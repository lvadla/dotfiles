{
  "files.exclude": {
    "**/.history": true,
    "USE_GITIGNORE": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/coverage": true,
    "**/dist": true,
    "**/build": true,
    "**/.build": true,
    "**/.gh-pages": true
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.statusBar.feedback.visible": false,
  "workbench.sideBar.location": "left",
  "workbench.editor.tabCloseButton": "off",
  "workbench.editor.tabSizing": "shrink",
  "workbench.activityBar.visible": false,
  "workbench.colorCustomizations": {},
  "workbench.settings.enableNaturalLanguageSearch": false,
  "terminal.integrated.shell.osx": "/bin/bash",
  "window.title": "${dirty} ${activeEditorShort} ${} ${separator} ${appName} ${separator} ${rootName} ${dirty}",
  "window.titleBarStyle": "custom",
  "window.nativeTabs": false,
  "editor.formatOnSave": true,
  "editor.rulers": [
    100
  ],
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "entity.name.function",
          "support.function"
        ],
        "settings": {
          "fontStyle": "bold"
        }
      }
    ]
  },
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorBlinking": "phase",
  "editor.smoothScrolling": true,
  "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.maxColumn": 200,
  "editor.minimap.showSlider": "always",
  "editor.snippetSuggestions": "top",
  "editor.scrollBeyondLastLine": false,
  "editor.tabSize": 2,
  "editor.fontFamily": "'FiraCode-Retina', 'Monaco', 'Courier New'",
  "editor.fontSize": 18,
  "editor.fontWeight": "400",
  "editor.fontLigatures": true,
  "editor.quickSuggestionsDelay": 5,
  "editor.multiCursorModifier": "alt",
  "vscode_custom_css.policy": true,
  "vscode_custom_css.imports": [
    "file:///Users/lancevadla/Documents/styles.css"
  ],
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  // "prettier.singleQuote": true,
  // "prettier.eslintIntegration": false,
  // "prettier.printWidth": 200,
  // "prettier.tabWidth": 2,
  // "prettier.jsxBracketSameLine": false,
  // "prettier.useTabs": false,
  // "prettier.proseWrap": "always",
  // "prettier.trailingComma": "es5",
  "explorer.confirmDragAndDrop": false,
  "colorize.enable_search_variables": false,
  "eslint.autoFixOnSave": false,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.options": {
    "env": {
      "browser": true,
      "jest/globals": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "no-debugger": "off"
    }
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "markdown.preview.scrollPreviewWithEditor": false,
  "markdown.preview.scrollEditorWithPreview": false,
  "window.zoomLevel": -1,
  "spellright.notificationClass": "warning",
  "spellright.ignoreFiles": [
    "**/.chunkwmrc",
    "~/.skhdrc",
    "**/.spellignore",
    "**/settings.json"
  ],
  "markdown.preview.fontSize": 24,
  "spellright.language": [
    "en"
  ],
  "spellright.documentTypes": [
    "markdown",
    "latex"
  ],
  "spellright.suggestionsInHints": false,
  "breadcrumbs.enabled": true,
  "gitlens.advanced.blame.delayAfterEdit": 0,
  "editor.renderWhitespace": "all",
  "editor.renderIndentGuides": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "terminal.explorerKind": "external",
  "terminal.integrated.cursorBlinking": true,
  "javascript.preferences.quoteStyle": "single",
  "javascript.referencesCodeLens.enabled": true,
  "path-intellisense.autoSlashAfterDirectory": true,
  "files.trimTrailingWhitespace": true,
  "typescript.experimental.useSeparateSyntaxServer": true,
  "typescript.surveys.enabled": false,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
  },
  "emmet.triggerExpansionOnTab": true,
  "gitlens.advanced.similarityThreshold": 67,
  "gitlens.advanced.telemetry.enabled": false,
  "gitlens.codeLens.authors.command": "gitlens.showQuickCommitDetails",
  "gitlens.defaultGravatarsStyle": "identicon",
  "gitlens.blame.format": "${author|10?} ${agoOrDate|14-}",
  "gitlens.blame.heatmap.location": "left",
  "gitlens.heatmap.ageThreshold": "20",
  "gitlens.hovers.annotations.over": "annotation",
  "gitlens.currentLine.format": "${message}",
  "gitlens.heatmap.toggleMode": "window",
  "terminal.integrated.fontSize": 11,
  "dateTime.customFormat": "dd MMMM Mo - HH:mm:ss",
  "vscode_custom_css.statusbar": false,
  "diffEditor.renderSideBySide": false,
  "jest.disabledWorkspaceFolders": [
    "fleet-and-schedule-management",
    "component-library",
    "opssuite-all"
  ],
  "jest.rootPath": "./opssuite-react-components",
  "jest.debugCodeLens.showWhenTestStateIn": [
    "fail"
  ],
  "jest.runAllTestsFirst": false,
  "workbench.colorTheme": "Bluloco Dark",
  "jest.enableInlineErrorMessages": false
}